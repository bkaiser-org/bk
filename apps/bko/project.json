{
  "name": "bko",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "bk",
  "sourceRoot": "apps/bko/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/bko",
        "index": "apps/bko/src/index.html",
        "browser": "apps/bko/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/bko/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/bko/src/assets",
          {
            "glob": "**/*.svg",
            "input": "node_modules/ionicons/dist/ionicons/svg",
            "output": "./svg"
          }
        ],
        "styles": [
          "apps/bko/src/styles.scss",
          {
            "input": "apps/bko/src/theme/variables.scss"
          }
        ],
        "allowedCommonJsDependencies": [
          "countries-list",
          "i18n-iso-countries",
          "crypto-js",
          "flat",
          "rfdc",
          "mime",
          "file-saver"
        ], 
        "scripts": [],
        "server": "apps/bko/src/main.server.ts",
        "prerender": true,
        "ssr": {
          "entry": "apps/bko/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1mb",
              "maximumError": "4mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "./apps/bko/src/environments/environment.ts",
              "with": "./apps/bko/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "bko:build:production"
        },
        "development": {
          "buildTarget": "bko:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "bko:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/bko/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "bko:build",
        "port": 4200,
        "staticFilePath": "dist/apps/bko/browser",
        "spa": true
      }
    }
  }
}
